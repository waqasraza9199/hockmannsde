{# ThemeWare: CMS block "Two columns 3-9" #}

{% block block_twt_grid_two_column_3_9 %}

    {% block block_twt_grid_two_column_3_9_left %}
        {% set element = block.slots.getSlot('left') %}

        <div class="column-1 col-12 col-md-3 twt-column left first">
            {% block block_twt_grid_two_column_3_9_left_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_twt_grid_two_column_3_9_right %}
        {% set element = block.slots.getSlot('right') %}

        <div class="column-2 col-12 col-md-9 twt-column right last">
            {% block block_twt_grid_two_column_3_9_right_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

{% endblock %}
