{# ThemeWare: CMS block "Two columns 7-5" #}

{% block block_twt_grid_two_column_7_5 %}

    {% block block_twt_grid_two_column_7_5_left %}
        {% set element = block.slots.getSlot('left') %}

        <div class="column-1 col-12 col-md-7 twt-column left first">
            {% block block_twt_grid_two_column_7_5_left_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_twt_grid_two_column_7_5_right %}
        {% set element = block.slots.getSlot('right') %}

        <div class="column-2 col-12 col-md-5 twt-column right last">
            {% block block_twt_grid_two_column_7_5_right_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

{% endblock %}
