{% set columns = 5 %}
{% set id = element.id %}

{% if isInSidebar %}
    <div class="bg-light py-4 px-2 text-center">
        {{ "CmsElements.callToAction.elementError"|trans|sw_sanitize }}
    </div>
{% else %}
    <div class="cta-banner-category-grid version-five col-12">
        <div class="row">
            <div class="col-lg-9">
                <div class="row pb-lg-2">
                    <div class="col-lg-6 pr-lg-2 mb-3 mb-lg-0">
                        {% set element = block.slots.getSlot('one') %}
                        <div class="pb-lg-2 mb-3 mb-lg-0" data-cms-element-id="{{ id }}">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>

                        {% set element = block.slots.getSlot('two') %}
                        <div class="pt-lg-2" data-cms-element-id="{{ id }}">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>
                    </div>

                    {% set element = block.slots.getSlot('three') %}
                    <div class="col-lg-6 px-lg-2 mb-3 mb-lg-0" data-cms-element-id="{{ id }}">
                        {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                    </div>
                </div>
                <div class="row pt-lg-2 mb-3 mb-lg-0">
                    {% set element = block.slots.getSlot('five') %}
                    <div class="col-lg-3 pr-lg-2 mb-3 mb-lg-0" data-cms-element-id="{{ id }}">
                        {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                    </div>

                    {% set element = block.slots.getSlot('six') %}
                    <div class="col-lg-6 px-lg-2 mb-3 mb-lg-0" data-cms-element-id="{{ id }}">
                        {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                    </div>

                    {% set element = block.slots.getSlot('seven') %}
                    <div class="col-lg-3 px-lg-2" data-cms-element-id="{{ id }}">
                        {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                    </div>
                </div>
            </div>

            {% set element = block.slots.getSlot('four') %}
            <div class="col-lg-3 pl-lg-2" data-cms-element-id="{{ id }}">
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            </div>
        </div>
    </div>
{% endif %}


